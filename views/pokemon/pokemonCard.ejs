<% pokemon.map(poke => { %> <% if(poke) { %>

<div id="<%= poke.dexnr %>">
  <h2>#<%= poke.dexnr %> || <%= poke.name %></h2>
  <p><%= poke.primary_type%> | <%= poke.secondary_type %></p>
  <p>Gen: <%= poke.generation %></p>
  <p>Can Megaevolve? <%= poke.mega_evolution %></p>
  <form action="/pokemon/<%= poke.dexnr %>" method="get">
    <button type="submit">Detail View</button>
  </form>
</div>
<% } %> 
<% }) %>
